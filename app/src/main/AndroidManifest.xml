<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.floclone">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/splash_image"
        android:label="@string/app_name"
        android:roundIcon="@drawable/splash_image"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SettingsActivity" android:parentActivityName=".UserMainScreen" android:launchMode="singleTop"></activity>
        <activity android:name=".SymptomsActivity" />
        <activity android:name=".ReportsActivity" android:parentActivityName=".UserMainScreen" android:launchMode="singleTop"/>
        <activity
            android:name=".ProfileActivity"
            android:launchMode="singleTop"
            android:parentActivityName=".UserMainScreen" />
        <activity
            android:name=".UserMainScreen"
            android:launchMode="singleTop" />
        <activity android:name=".PredictingCycleSplashScreen" />
        <activity android:name=".TrackMyCycleActivity" />
        <activity android:name=".TrackMyCycle" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>